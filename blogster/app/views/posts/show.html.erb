<div class="inline-container" id="post-header">
  <h1 id="title"> <%= @post.title %> </h1>
  <i id="date"> <%= @post.created_at.strftime("%d/%m/%y") %> </i>
</div>
<p><%= @post.content %></p>
<%= link_to "Edit", edit_post_path(@post) %>
<%= link_to "Delete", @post, method: :delete, data: { confirm: "Are you sure?" } %>